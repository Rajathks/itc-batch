$(document).ready(function(){
	
	
	var postForm=$('#post-form');
	
	postForm.submit(function(e) {
		
		var emailField=$('#email',this);
		var messageField=$('#message',this);
		
		var email=emailField.val();
		var message=messageField.val();
		
		
	
		e.preventDefault();
		
	});
	
});





