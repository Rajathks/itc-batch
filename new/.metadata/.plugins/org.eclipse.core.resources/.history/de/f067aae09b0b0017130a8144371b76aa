package simple;

class Person{
	String name;
	int age;
	public Person(String name,int age) {
		this.name=name;
		this.age=age;
	}
	void sayName(){
		String name="itc";
		System.out.println("im "+this.name);
	}
	void sayAge(){
		System.out.println("im "+this.age+"old");
	}
}













public class Demo {
	
	public static void main(String[] args) {
		
		Person person=new Person();
		person.name="Nag";
		
		person.sayName();
		
	}

}
