package com.app.model;

public class Person {

	private String name;
	private int age;

	private Mobile dog;

	public Mobile getMobile() {
		return dog;
	}

	public void setDog(Mobile dog) {
		this.dog = dog;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

}
