class Abc{
	
	static int sta=1;
	int ins=2;
	
	void insMethod(){
		System.out.println(sta);
		System.out.println(ins);
	}
	
	static void staMethod(){
		System.out.println(sta);
		System.out.println(ins);
	}
	
}



public class Demo {

	public static void main(String[] args) {

		// Student.tnrName="Nag";
		// Student.teach();
		
		Abc abc=new Abc();
		abc.ins=100;
		abc.insMethod();

	}

}
